{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/domain/*": [
        "./src/domain/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/types/*": [
        "./src/domain/types/*"
      ],
      "@/entities/*": [
        "./src/domain/entities/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
